{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "UnPivot",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "output",
						"type": "DatasetReference"
					},
					"name": "unpivotdata"
				}
			],
			"transformations": [
				{
					"name": "unpivot1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ROLL_NO as string,",
				"          English as string,",
				"          History as string,",
				"          Maths as string,",
				"          Physics as string,",
				"          Science as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 unpivot(output(",
				"          SUBJECT as string,",
				"          MARKS as string",
				"     ),",
				"     ungroupBy(ROLL_NO),",
				"     lateral: true,",
				"     ignoreNullPivots: false,",
				"     partitionBy('hash', 1)) ~> unpivot1",
				"unpivot1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> unpivotdata"
			]
		}
	}
}